.gitignore{
# Ignore node modules
node_modules/
# Ignore build output
dist/
build/
# Ignore environment variable files     
.env
.env.local
.env.*.local
# Ignore logs
logs/
*.log               
npm-debug.log*
yarn-debug.log* 
yarn-error.log*
# Ignore OS generated files     
.DS_Store
Thumbs.db

# Ignore IDE specific files
.vscode/
.idea/
*.suo
*.ntvs*
*.njsproj
*.sln   
# Ignore coverage directory
coverage/   
# Ignore temporary files
*.tmp

# Ignore lock files
package-lock.json
yarn.lock
pnpm-lock.yaml
}
